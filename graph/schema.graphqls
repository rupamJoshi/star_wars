# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Query {
  searchCharacter(name: String!): Character
  getSavedResults: [FavoriteCharacter]
}

type Mutation {
  saveSearchResult: FavoriteCharacter
}

type Character {
  name: String!
  films:[String!]
  Vehicles: [String!]
  SavedAt: String
}

type FavoriteCharacter {
  id: String!
  name: String!
  films:[String!]
  Vehicles: [String!]
  SavedAt: String!
}






